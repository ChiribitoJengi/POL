<FilesMatch "\.(html|css|js)$">
SetOutputFilter DEFLATE
</FilesMatch>

Header unset ETag
FileETag None

ExpiresActive On
expiresbytype image/gif "access plus 2 days"
expiresbytype image/jpg "access plus 2 days"
expiresbytype text/txt "access plus 2 days"
expiresbytype text/js "access plus 2 days" 